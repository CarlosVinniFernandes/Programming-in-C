//media
#include <stdio.h>

int main()
{
    int num1, num2, num3, resultado, media;
    printf("Digite a primeira nota:");
    scanf("%d", &num1);
    
    printf("Digite a segunda nota:");
    scanf("%d", &num2);
    
    printf("Digite a terceira nota:");
    scanf("%d", &num3);
    
    media = (num1 + num2 + num3)/3;
    printf("A média dos números é: %d", media);
    return 0;
}
